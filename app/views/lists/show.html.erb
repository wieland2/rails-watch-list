<%= link_to @list.name, lists_path %>


<% @list.bookmarks.each do |item| %>
<p><%= item.movie.title%></p>
<p><%= item.movie.overview%></p>
<p><%= item.movie.rating%></p>
<p><%= item.comment%></p>
<%= link_to "Delete",
  list_path(@list),
  data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
%>

<% end %>

<p><%= link_to "Return to List", lists_path %></p>
<p><%= link_to "Add a Movie", new_list_bookmark_path(@list) %></p>
